{
    "name": "cammanderson/mmb",
    "description": "Minimalist markdown blogging tool",
    "keywords": ["blog", "markdown", "coding"],
    "license": "LGPL-3.0",
    "authors": [
        {
            "name": "Cameron Manderson",
            "email": "cameronmanderson@gmail.com",
            "role": "developer"
        }
    ],
    "type": "library",
    "autoload": {
        "psr-4": {
            "MMB\\": ""
        }
    },
    "require": {
        "php": ">=5.4.0",
        "silex/silex": "~1.2",
        "symfony/yaml": "~2.4",
        "symfony/process" : "dev-master",
        "erusev/parsedown": "dev-master",
        "deralex/yaml-config-service-provider": "~1",
        "symfony/finder" : "~2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1"
    },
    "suggest": {
        "erusev/parsedown-extra": "dev-master"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}